<script type="text/javascript">
		(function() {
			function SetTxtWordsNoValue() {
				var searchtxt = $("txtwordsNav");
				if (searchtxt.value == "书名/作者/标签") {
						searchtxt.value = "";
						searchtxt.style.color = "black";
				}
			};
			function SetTxtWordsValue() {
				var searchtxt = $("txtwordsNav");
				if (searchtxt.value == "") {
					searchtxt.value = "书名/作者/标签";
					searchtxt.style.color = "Gray";
				};
			};
			eventBind($("txtwordsNav"),"click",SetTxtWordsNoValue);
			eventBind($("txtwordsNav"),"blur",SetTxtWordsValue);
		})();
		
		var caihongkefuOsOpen = function(gmOS, sOS, tOS) {
			if (!window.oswin || window.oswin.closed) {
				var callBack = function(res) {
					var o = eval(res);
					if (o.code == 1)    //起点盟主问题
						tOS = o.msg;
			
						window.oswin = window.open('http://os.sdo.com/default.aspx?gm=' + gmOS + '&intype=' + sOS + '&t=' + escape(tOS), '', 'toolbar=no,location=no,left=' + (window.screen.width - 640) / 2 + ',top=' + (window.screen.height - 480) / 2 + ',directories=no,scrollbars=no,menubar=no,width=465,height=455,resizable=no,status=no');
					};
					if (!checkLoginByCookie())
						callBack({ code: 0 });
					else
						MyAjax.CheckUserIsChief(callBack);
					}
			else {
				alert('您已经打开了一个对话');
			}
		};

	</script>
